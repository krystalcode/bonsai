        }
    ],
    "require": {
        "php": ">=5.4.0",
        "kriswallsmith/buzz"    : "^1.0",
        "mailgun/mailgun-php"   : "dev-2.8.1-headers",
        "guzzlehttp/psr7"       : "^1.6"
    },
    "autoload": {
        "psr-4": {
